@model List<Movie>

<div class="slick-multiItemSlider">
	@foreach(var item in Model)
	{
		<div class="movie-item ">
			<a href="#">
				<div class="mv-img">
			
					<div style="padding:5px; width:285px; height:437px;">
					<img src="@item.MovieImage" style="width:100%; height:100%; object-fit:cover;" alt="">
				</div>
			
			</div>	</a>
			<div class="title-in">
				<div class="cate">
					<span class="blue"><a href="#">Sci-fi</a></span>
				</div>
				<h6><a href="#">@item.MovieName</a></h6>
				<p><i class="ion-android-star"></i><span>7.4</span> /10</p>
			</div>
		</div>
	}
	
	
</div>