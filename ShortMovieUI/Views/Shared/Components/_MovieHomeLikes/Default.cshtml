@model List<Movie>

<div id="tab3" class="tab">
	<div class="row">
		<div class="slick-multiItem">
			@foreach(var item in Model)
			{
				<div class="slide-it">
					<div class="movie-item">
						<div class="mv-img">
							<div style="padding:5px; width:185px; height:284px;">
								<img src="@item.MovieImage" alt="" style="width:100%; height:100%; object-fit:cover;">
							</div>
						</div>
						<div class="hvr-inner">
							<a href="moviesingle.html"> Read more <i class="ion-android-arrow-dropright"></i> </a>
						</div>
						<div class="title-in">
							<h6><a href="#">@item.MovieName</a></h6>
							<p><i class="ion-android-star"></i><span>7.4</span> /10</p>
						</div>
					</div>
				</div>
			}
			
			
		</div>
	</div>
</div>